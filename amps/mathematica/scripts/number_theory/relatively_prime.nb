Cell[BoxData[
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"i", "=", "10000"}], ",", 
   RowBox[{"i", "<", "50000"}], ",", 
   RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"list", "=", 
     RowBox[{"RandomInteger", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1000"}], ",", "1000"}], "}"}], ",", 
       RowBox[{"{", "2", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"addcoprime", "=", 
     RowBox[{"RandomChoice", "[", 
      RowBox[{"{", 
       RowBox[{"True", ",", "False"}], "}"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{"addcoprime", ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"list", "=", 
        RowBox[{"Append", "[", 
         RowBox[{"list", ",", 
          RowBox[{
           RowBox[{
            RowBox[{"ResourceFunction", "[", "\"\<Shuffle\>\"", "]"}], "[", 
            RowBox[{
             RowBox[{
              RowBox[{
              "ResourceFunction", "[", "\"\<CoprimeIntegerList\>\"", "]"}], 
              "[", 
              RowBox[{"list", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", 
             "\"\<RandomSample\>\""}], "]"}], "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";"}]}], 
     "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"list", "=", 
     RowBox[{
      RowBox[{"ResourceFunction", "[", "\"\<Shuffle\>\"", "]"}], "[", 
      RowBox[{"list", ",", "\"\<RandomSample\>\""}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Export", "[", 
     RowBox[{
      RowBox[{
      "\"\</Users/hendrycks/Desktop/mesa/number_theory/relatively_prime/\>\"",
        "<>", 
       RowBox[{"ToString", "[", "i", "]"}], "<>", "\"\<.txt\>\""}], ",", " ", 
      
      RowBox[{"{", 
       RowBox[{
       "\"\<Problem:\\nAre the following numbers relatively prime (coprime)? \
$\>\"", "<>", 
        RowBox[{"ToString", "[", 
         RowBox[{"TeXForm", "[", "list", "]"}], "]"}], "<>", 
        "\"\<$.\\nAnswer:\\n$\>\"", "<>", 
        RowBox[{"ToString", "[", 
         RowBox[{"TeXForm", "[", 
          RowBox[{"CoprimeQ", " ", "@@", "list"}], "]"}], "]"}], "<>", 
        "\"\<$\>\""}], "}"}]}], "]"}]}]}], "\[IndentingNewLine]", 
