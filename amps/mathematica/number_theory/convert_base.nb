Cell["Convert x to base b.", "Text",
 CellChangeTimes->{{3.81810430145477*^9, 
  3.818104314350555*^9}},ExpressionUUID->"55ae4afe-af62-4d05-80b4-\
ec7f6bd62d31"],

Cell[BoxData[
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"i", "=", "10000"}], ",", 
   RowBox[{"i", "<", "30000"}], ",", 
   RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"x", "=", 
     RowBox[{"RandomChoice", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"RandomInteger", "[", 
         RowBox[{"{", 
          RowBox[{"2", ",", "3000"}], "}"}], "]"}], ",", 
        RowBox[{"Round", "[", 
         RowBox[{
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"0.001", ",", "1"}], "}"}], "]"}], ",", 
          RowBox[{"RandomChoice", "[", 
           RowBox[{"{", 
            RowBox[{"0.1", ",", "0.01", ",", "0.001"}], "}"}], "]"}]}], 
         "]"}]}], "}"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"b", "=", 
     RowBox[{"RandomChoice", "[", 
      RowBox[{"Delete", "[", 
       RowBox[{
        RowBox[{"Range", "[", 
         RowBox[{"2", ",", "36"}], "]"}], ",", " ", "9"}], "]"}], "]"}]}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{"Export", "[", 
     RowBox[{
      RowBox[{
      "\"\</Users/hendrycks/Desktop/mesa/number_theory/convert_base/\>\"", "<>", 
       RowBox[{"ToString", "[", "i", "]"}], "<>", "\"\<.txt\>\""}], ",", " ", 
      
      RowBox[{"{", 
       RowBox[{"\"\<Problem:\\nConvert $\>\"", "<>", 
        RowBox[{"ToString", "[", 
         RowBox[{"TeXForm", "[", "x", "]"}], "]"}], "<>", 
        "\"\<$ to base $\>\"", "<>", 
        RowBox[{"ToString", "[", 
         RowBox[{"TeXForm", "[", "b", "]"}], "]"}], "<>", 
        "\"\<$.\\nAnswer:\\n$\>\"", "<>", 
        RowBox[{"ToString", "[", 
         RowBox[{"TeXForm", "[", 
          RowBox[{"BaseForm", "[", 
           RowBox[{"x", ",", "b"}], "]"}], "]"}], "]"}], "<>", "\"\<$\>\""}], 
       "}"}]}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]], "Input",
